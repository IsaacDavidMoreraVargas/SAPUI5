<mvc:View controllerName="dtt.SAPUI5.controller.App" 
        xmlns="sap.m" 
        xmlns:mvc="sap.ui.core.mvc" 
        displayBlock="true">  
    <Page id="_IDGenPage12" title="{i18n>appTitle}">
        <headerContent>
            <Button id="HelloDialogHeader" 
            icon="sap-icon://action" 
            press=".onOpenDialogHeader" 
            class="sapUiSmallMarginEnd" />
        </headerContent>
        <content>
            <mvc:XMLView id="_IDGenMXLView1" viewName="dtt.SAPUI5.view.HelloPanel" />
            <mvc:XMLView id="_IDGenMXLView2" viewName="dtt.SAPUI5.view.InvoicesList" />
        </content>
    </Page>
</mvc:View>
